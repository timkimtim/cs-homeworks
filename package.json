  "private": true,
  "dependencies": {
    "@fontsource/montserrat": "^4.5.13",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "sass": "^1.52.1"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.0",
    "@types/node": "^16.11.7",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-webpack-plugin": "^3.2.0",
    "prettier": "^2.4.1",
    "tailwindcss": "^3.3.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.1.2",
    "webpack": "^5.72.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "format": "prettier --write \"{src,types,scripts}/**/*.{json,{j,t}s{,x},scss,}\"",
    "lint": "npm run lint:ts",
    "lint:ts": "eslint \"{src,types}/**/*.ts{,x}\"",
    "lint-fix": "npm run format && npm run lint-fix:ts",
    "lint-fix:ts": "eslint \"{src,types}/**/*.ts{,x}\" --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache",
      "bash -c tsc"
    ]
  },
  "resolutions": {
    "react-scripts/postcss-preset-env/postcss-custom-properties": "^12.1.8"
  }
}
